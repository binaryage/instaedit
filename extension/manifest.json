{
  "name": "Instaedit",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Instaedit - pixel perfect web editor",
  "browser_action": {
    "default_icon": "icon/icon2.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": []
  },
  "permissions":  [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file://localhost/*"
  ]
}